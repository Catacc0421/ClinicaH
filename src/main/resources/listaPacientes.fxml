<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="367.0" prefWidth="526.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" prefWidth="525.0" text="Lista de Pacientes" textFill="#0f0f0f">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="tablaPacientes" prefHeight="310.0" prefWidth="479.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn text="Identificación" />
            <TableColumn text="Nombre" />
            <TableColumn text="Teléfono" />
            <TableColumn text="Correo" />
            <TableColumn text="Suscripción" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
